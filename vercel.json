{
  "rewrites": [
    { "source": "/api/data", "destination": "/api/data.js" }
  ],
  "headers": [
    {
      "source": "/api/data",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS" }
      ]
    }
  ]
}